      dbSetOrder(1)
      dbSeek(cKey)
      While !eof() .and. E1_FILIAL+E1_PREFIXO+E1_NUM == cKey
	    If LastKey()==286
	       @25,1 PSAY "** CANCELADO PELO OPERADOR **"
	       lContinua := .F.
	       Exit
	    Endif
	    nDup := nDup + 1
	    If nDup <= 4
	       aDupl[nDup,1] := AllTrim(Str(nDup))
	       aDupl[nDup,2] := E1_VENCTO
